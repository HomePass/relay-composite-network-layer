{
  "name": "relay-composite-network-layer",
  "description": "A network layer for relay which can work with multiple underlying network layers",
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/eyston/relay-composite-network-layer",
  "repository": "eyston/relay-composite-network-layer",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "babel src --out-dir lib/",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "merge-schemas": "babel-node ./scripts/mergeSchemas.js",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-register",
    "karma": "karma start",
    "start": "babel-node src/index.js"
  },
  "dependencies": {
    "immutable": "3.7.6",
    "lodash": "3.10.1"
  },
  "peerDependencies": {
    "react-relay": "0.6.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "./build/babelRelayPlugin"
    ]
  },
  "devDependencies": {
    "babel": "6.3.13",
    "babel-cli": "6.3.13",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-relay-plugin": "0.6.0",
    "babel-register": "6.3.13",
    "expect": "1.13.4",
    "graphql": "0.4.14",
    "graphql-relay": "0.3.6",
    "json-loader": "0.5.4",
    "karma": "0.13.15",
    "karma-babel-preprocessor": "6.0.1",
    "karma-chrome-launcher": "0.2.2",
    "karma-mocha": "0.2.1",
    "karma-webpack": "1.7.0",
    "mocha": "2.3.4",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "relay-local-schema": "0.3.0",
    "webpack": "1.12.9"
  }
}
